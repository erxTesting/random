git clone https://github.com/rw251/git-summary.git # tools
./git-summary/git-summary
git branch -a # list all branches
git checkout <branchname> # change to branch
git branch # sho current branch
git status
git add -A # add all untracked files
git commit -m "Commit me" -a # commit and add a message and all tracked files
git push
